{
  "$schema": "../../node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "defaultProject": "main",
  "projects": {
    "main": {
      "root": "projects/main",
      "sourceRoot": "projects/main/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/main",
            "index": "projects/main/src/index.html",
            "main": "projects/main/src/main.ts",
            "polyfills": "projects/main/src/polyfills.ts",
            "tsConfig": "projects/main/tsconfig.app.json",
            "aot": true,
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/main/src/assets",
              "projects/main/src/assets/img"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/main/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": {
                "styles": true,
                "scripts": true,
                "fonts": true
              },
              "sourceMap": {
                "scripts": false,
                "styles": false,
                "vendor": false,
                "hidden": false
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "8kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/main/src/environments/environment.ts",
                  "with": "projects/main/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": {
                "styles": false,
                "scripts": false,
                "fonts": false
              },
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": true,
                "hidden": true
              },
              "namedChunks": false,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "main:build:production"
            },
            "development": {
              "browserTarget": "main:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "main:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/main/src/test.ts",
            "polyfills": "projects/main/src/polyfills.ts",
            "tsConfig": "projects/main/tsconfig.spec.json",
            "karmaConfig": "projects/main/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/main/src/assets",
              "projects/main/src/assets/img"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/main/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "ks-lib": {
      "projectType": "library",
      "root": "projects/ks-lib",
      "sourceRoot": "projects/ks-lib/src",
      "prefix": "ks-lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ks-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ks-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ks-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ks-lib/src/test.ts",
            "tsConfig": "projects/ks-lib/tsconfig.spec.json",
            "karmaConfig": "projects/ks-lib/karma.conf.js"
          }
        }
      }
    }
  }
}
